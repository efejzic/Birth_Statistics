> boxplot(tgrams ~ marital, data = ncb50,
+         main = "Boxplot for Tgrams by Marital",
+         xlab = "Marital (1 = Married, 2 = Not Married)â€,
+         ylab = "Tgrams")

	Quartiles

> quantiles_by_marital_1 <- quantile(ncb50$tgrams[ncb50$marital == 1], c(0.25, 0.5, 0.75))

